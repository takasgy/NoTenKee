<?xml version="1.0" encoding="UTF-8"?>
<report>
	<book>
		<report_name>表形式帳票Ｎ１</report_name>
		<type>shingle</type>
		<reference_format>A1</reference_format>
		<template_name>templTEST_REPORT_N1.xlsx</template_name>
		<file_name>TEST_REPORT_N1_yyyyMMddHHmmss_000.xlsx</file_name>
		<break_key>ブレイクキー1,ブレイクキー2</break_key>
		<break_action>newbook</break_action>
		<max_sheet>5</max_sheet>
		<sheet_name>P_{page}</sheet_name>
		<template_sheet>template</template_sheet>
	</book>
	<csv_info>
		<delimiter>,</delimiter>
		<quote></quote>
		<codeset>utf-8</codeset>
		<csv_header>false</csv_header>
	</csv_info>
	<cells>
		<cell>
			<name>請求締日</name>
			<csv_key>請求締日</csv_key>
			<position>1</position>
			<positionX>B</positionX>
			<positionY>1</positionY>
			<data_type>Datetime</data_type>
			<data_format>yyyymmdd</data_format>
			<cell_format>yyyy/mm/dd</cell_format>
		</cell>
		<cell>
			<name>取引先名</name>
			<csv_key>取引先名</csv_key>
			<position>2</position>
			<positionX>B</positionX>
			<positionY>2</positionY>
			<data_type>String</data_type>
		</cell>
		<cell>
			<name>ブレイクキー1</name>
			<csv_key>ブレイクキー1</csv_key>
			<position>3</position>
			<positionX>C</positionX>
			<positionY>4</positionY>
			<data_type>String</data_type>
		</cell>
		<cell>
			<name>ブレイクキー2</name>
			<csv_key>ブレイクキー2</csv_key>
			<position>4</position>
			<positionX>F</positionX>
			<positionY>4</positionY>
			<data_type>String</data_type>
		</cell>
		<cell>
			<name>合計金額</name>
			<csv_key>合計金額</csv_key>
			<position>11</position>
			<positionX>H</positionX>
			<positionY>17</positionY>
			<data_type>Long</data_type>
			<cell_format>#,##0</cell_format>
		</cell>
	</cells>
	<row_info>
		<name>_ROW</name>
		<list_type>N</list_type>
		<n_row_max>5</n_row_max>
		<n_incremental>2</n_incremental>
		<z_row_max>1</z_row_max>
		<z_incremental>0</z_incremental>
	</row_info>
	<row_cells>
		<row_cell>
			<name>取引日付</name>
			<csv_key>取引日付</csv_key>
			<position>5</position>
			<positionX>A</positionX>
			<positionY>7</positionY>
			<data_type>Datetime</data_type>
			<data_format>yyyymmdd</data_format>
			<cell_format>yyyy/mm/dd</cell_format>
		</row_cell>
		<row_cell>
			<name>商品コード</name>
			<csv_key>商品コード</csv_key>
			<position>6</position>
			<positionX>B</positionX>
			<positionY>8</positionY>
			<data_type>String</data_type>
		</row_cell>
		<row_cell>
			<name>商品名</name>
			<csv_key>商品名</csv_key>
			<position>7</position>
			<positionX>B</positionX>
			<positionY>7</positionY>
			<data_type>String</data_type>
		</row_cell>
		<row_cell>
			<name>数量</name>
			<csv_key>数量</csv_key>
			<position>8</position>
			<positionX>D</positionX>
			<positionY>7</positionY>
			<data_type>Int</data_type>
			<cell_format>#,##0</cell_format>
		</row_cell>
		<row_cell>
			<name>単価</name>
			<csv_key>単価</csv_key>
			<position>9</position>
			<positionX>F</positionX>
			<positionY>7</positionY>
			<data_type>Double</data_type>
			<cell_format>#,##0.00</cell_format>
		</row_cell>
		<row_cell>
			<name>金額</name>
			<csv_key>金額</csv_key>
			<position>10</position>
			<positionX>H</positionX>
			<positionY>7</positionY>
			<data_type>Long</data_type>
			<cell_format>#,##0</cell_format>
		</row_cell>
	</row_cells>
</report>